print_Puzzle:
loop:
beq $t2, 81, finishPrint
addi $t0, $zero, 0xffff8000
add $t0, $t0, $t3
lb $t1, ($t0)
addi $t3, $t3, 1
div $t5, $t3, 9
mfhi $t6
addi $v0, $zero, 1
add $a0, $zero, $t1
syscall
addi $t2, $t2, 1
beq $t6, $zero, space
j loop
finishPrint:
j solve
space:
addi $a0, $0, 0xA #ascii code for LF, if you have any trouble try 0xD for CR.
addi $v0, $0, 0xB #syscall 11 prints the lower 8 bits of $a0 as an ascii character.
syscall
j loop

solve:
